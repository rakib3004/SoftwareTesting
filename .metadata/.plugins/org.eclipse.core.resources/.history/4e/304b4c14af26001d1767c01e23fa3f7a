package UnitTest;

import static org.junit.Assert.*;

import org.junit.Test;

public class MathTesting {

	@Test
	public void test() {
		MainClass unitOne = new MainClass();
		
		int result1 = unitOne.square(2);
		int result2 = unitOne.square(3);
		int result3 = unitOne.square(4);
		int result4 = unitOne.square(5);
		int result5 = unitOne.square(6);

		
		assertEquals(4,result1);
		assertEquals(9,result2);
		assertEquals(16,result3);
		assertEquals(25,result4);
		assertEquals(16,result5);
		
	}

}
